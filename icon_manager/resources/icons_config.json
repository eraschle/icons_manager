{
    "rules": {
        "equals": "Are the rule values equals with the current attribute",
        "contains": "Does the attribute values contains the rule values",
        "not_contains": "Does the attribute values NOT contains the rule values"
    },
    "template": {
        "config_name": {
            "icon": "icon name > must exists in icon_path (not case sensitive)",
            "apply_to_root": "'true' or 'false' apply on root search folder >> default: true",
            "order": "Integer number to adjust order of config execution. The smaller the earlier it will be executed >> default 5",
            "rules": [
                {
                    "attribute": "'path' or 'name'",
                    "rule_to_apply": [
                        "Values to check"
                    ],
                    "operator": "'any' or 'all' > default: any",
                    "case_sensitive": "'true' or 'false' for rule >> default: false"
                }
            ]
        }
    },
